<h1><%= title %></h1>
<form method="post" action="/contact-us">
    <label for="firstName">First Name*</label><br/>
    <input type="text" id="firstName" name="firstName" value="<%= values.firstName %>" /><br/>

    <% if (validationMessages.firstName) { %>
        <span style="color: red;"><%= validationMessages.firstName %></span><br/><br/>
    <% } %>

    <label for="lastName">Last Name*</label><br/>
    <input type="text" id="lastName" name="lastName" value="<%= values.lastName %>" /><br/>

    <label for="email">Email*</label><br/>
    <input type="text" id="email" name="email" value="<%= values.email %>" /><br/>

    <label for="message">Message</label><br/>
    <textarea id="message" name="message"><%= values.message %></textarea><br/>

    <input type="submit" value="Contact Us"/>
</form>